<!DOCTYPE html>
<html>
	
	<head>
	  	<title>Shops</title>
	  	<%= stylesheet_link_tag    "application", "font-awesome", media: "all", "data-turbolinks-track" => true %>
	  	<%= javascript_include_tag "application", "bootstrap-growl.min", "data-turbolinks-track" => true %>
	  	<script src="//maps.google.com/maps/api/js?v=3.18&sensor=false&client=&key=&libraries=geometry&language=&hl=&region="></script> 
		<script src="//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js"></script>
		<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/infobox/1.1.9/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
	  	<%= csrf_meta_tags %>
	</head>
	
	<body>
		
		<!-- REGION : Top Navigation Menu Bar -->
		
		<div class="navbar navbar-primary navbar-fixed-top">
			<div class="container-fluid header">
				<%= yield :tabs %>
			</div>
		</div>
		
		<!-- END REGION : Top Navigation Menu Bar -->
		
		<!-- REGION : Container -->
		
		<div class="container-fluid container-fix">
			
			<!-- REGION : Content -->
			
			<%= yield %>
			
			<!-- END REGION : Content -->
			
		</div>
		
		<!-- END REGION : Container -->
		
		<!-- REGION : Modal Dialog Template -->
		
		<div class="modal fade" id="divModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
			<div class="modal-dialog modal-lg">
				<div class="modal-content">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
							&times;
						</button>
						<h4 class="modal-title"></h4>
					</div>
					<div class="modal-body">

					</div>
					<div class="modal-footer">
						<div class="pull-left pad10">
							<button type="button" title="Cancel Operation" data-dismiss="modal" class="btn btn-default helper-font-12"
							style="margin: 0px 5px">
								<i class="fa fa-reply helper-font-12"></i>
								<span style="padding-left: 10px;">Cancel</span>
							</button>
						</div>
						<div class="pull-right pad10">
							<button type="button" title="Proceed with the Operation" class="btn btn-success helper-font-12"
							style="margin: 0px 5px">
								<i class="fa fa-mail-forward helper-font-12"></i>
								<span style="padding-left: 10px;" class="btnModal">Proceed</span>
							</button>
						</div>
					</div>
				</div>
				<!-- /.modal-content -->
			</div>
			<!-- /.modal-dialog -->
		</div>
		
		<!-- END REGION : Modal Dialog Template -->
		
		<!-- REGION : Loading Helper Template -->

		<div id="divLoader" class="Loading" align="center">
			<p class="text-primary">
				Loading, Please Wait for a while...
			</p>
			<br />
		</div>
		<div id="divOverlay" class="Overlay"></div>
		
		<!-- END REGION : Loading Helper Template -->
		
	</body>
</html>
